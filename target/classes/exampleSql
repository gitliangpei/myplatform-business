CREATE TABLE WH_GRAPHIC_TEXT_GROUP
(
  ID            VARCHAR2(36 BYTE),
  GROUP_TYPE    VARCHAR2(3 BYTE),
  CREATE_BY     VARCHAR2(36 BYTE),
  CREATE_DATE   DATE,
  UPDATE_BY     VARCHAR2(36 BYTE),
  UPDATE_DATE   DATE,
  DEL_FLAG      VARCHAR2(3 BYTE),
  OFFICE_ID     VARCHAR2(50 BYTE),
  STATUS        VARCHAR2(3 BYTE),
  GROUP_NAME    VARCHAR2(50 BYTE),
  RELEASE_DATE  DATE
)
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON TABLE WH_GRAPHIC_TEXT_GROUP IS '微信图文组';

COMMENT ON COLUMN WH_GRAPHIC_TEXT_GROUP.ID IS '图文组ID';

COMMENT ON COLUMN WH_GRAPHIC_TEXT_GROUP.GROUP_TYPE IS '图文组类型（1代表政策动态；2代表区域动态；3代表内部动态）';

COMMENT ON COLUMN WH_GRAPHIC_TEXT_GROUP.CREATE_BY IS '创建者';

COMMENT ON COLUMN WH_GRAPHIC_TEXT_GROUP.CREATE_DATE IS '创建时间';

COMMENT ON COLUMN WH_GRAPHIC_TEXT_GROUP.UPDATE_BY IS '更新者';

COMMENT ON COLUMN WH_GRAPHIC_TEXT_GROUP.UPDATE_DATE IS '更新时间';

COMMENT ON COLUMN WH_GRAPHIC_TEXT_GROUP.DEL_FLAG IS '删除标记（代表未删除；1代表删除）';

COMMENT ON COLUMN WH_GRAPHIC_TEXT_GROUP.OFFICE_ID IS '所属部门ID';

COMMENT ON COLUMN WH_GRAPHIC_TEXT_GROUP.STATUS IS '0草稿，1已发布';

COMMENT ON COLUMN WH_GRAPHIC_TEXT_GROUP.GROUP_NAME IS '图文组名称';

COMMENT ON COLUMN WH_GRAPHIC_TEXT_GROUP.RELEASE_DATE IS '发布时间';

